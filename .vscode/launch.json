    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "编译",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "env": {
                "NODE_ENV": "develop",
                "TARGET_FOLDER": "${workspaceFolder}/大数相乘和相加"
            },
            "port": 9229,
            // "preLaunchTask": "",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动",
            "program": "${workspaceFolder}/.cache/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        // {
